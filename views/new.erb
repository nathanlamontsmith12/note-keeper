
<%if @user %>
<h3>Add Note for <%=@user.username%> </h3>
<br />
<form action="/notes/<%=@user.id%>" method="POST">
	<input name="content" value="" placeholder="Enter note...">
	<button>Add Note</button>
</form>

<% else %>
	<h3> Uh oh... there seems to be a problem! Return <a href="/">HOME</a></h3>
<% end %>
