<h3>Select user: </h3>

<ul>

	<% if @all_users and @all_users.length > 0 %>
		<% @all_users.each do |user| %>
			<li> 
				<a href="/notes/<%=user.id%>"> <%= user.username %> </a> 
				<br />
			</li>
			<br />
		<% end %>
	<% end %>	
</ul>

